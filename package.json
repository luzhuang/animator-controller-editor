{
  "name": "galacean-animator-controller",
  "version": "0.1.0",
  "description": "A standalone animator controller editor for Galacean Engine",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./adapters/galacean-editor": {
      "types": "./dist/adapters/galacean-editor/index.d.ts",
      "import": "./dist/adapters/galacean-editor/index.mjs",
      "require": "./dist/adapters/galacean-editor/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/cjs/index.js",
    "module": "dist/es/index.js",
    "types": "types/src/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/es/index.js",
        "types": "./types/src/index.d.ts",
        "require": "./dist/cjs/index.js"
      },
      "./adapters/galacean-editor": {
        "import": "./dist/es/adapters/galacean-editor/index.js",
        "types": "./types/adapters/galacean-editor/index.d.ts",
        "require": "./dist/cjs/adapters/galacean-editor/index.js"
      }
    }
  },
  "sideEffects": [
    "src/stores/x6-nodes.tsx"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build:types": "npm run clean:dts && tsc --emitDeclarationOnly --declaration --outDir types",
    "build:all": "npm run build && npm run build:types",
    "prepublishOnly": "npm run build:all",
    "clean": "rimraf dist types",
    "clean:dts": "rimraf types",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "play": "vite examples --config examples/vite.config.ts",
    "play:build": "vite build examples --config examples/vite.config.ts"
  },
  "keywords": [
    "galacean",
    "animator",
    "controller",
    "state-machine",
    "editor",
    "react",
    "3d"
  ],
  "author": "Galacean Team",
  "license": "MIT",
  "files": [
    "dist",
    "types",
    "src"
  ],
  "peerDependencies": {
    "@galacean/engine": ">=1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@antv/x6": "^2.18.1",
    "@antv/x6-react-shape": "^2.0.7",
    "@tabler/icons-react": "^3.12.0",
    "ahooks": "^3.7.8",
    "es-toolkit": "^1.21.0",
    "mobx": "^6.10.0",
    "mobx-react-lite": "^4.0.5",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/blocks": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "http-server": "^14.1.1",
    "playwright": "^1.53.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "serve": "^14.2.4",
    "storybook": "^7.6.7",
    "tsup": "^8.2.4",
    "typescript": "^5.7.0",
    "vite": "^5.1.4"
  },
  "packageManager": "pnpm@9.2.0"
}